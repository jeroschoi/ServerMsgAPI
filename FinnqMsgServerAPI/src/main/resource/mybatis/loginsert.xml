<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.ogr//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.gw.msg.mapper.insert">
	<insert id="insertSlackMsg" parameterMap="Map">
	INSERT INTO MESSAGE_LOG(
	 MSG_CONTENT
	,SVC
	,REG_DTTM
	)
	VALUES(
	#{MSG_CONTENT}
	,#{SVC}
	,#{REG_DTTM}
	)
	</insert>
</mapper>